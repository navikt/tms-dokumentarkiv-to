---
import OnePagerContainerV2 from "../../components/flat-v2/container/OnePagerContainerV2";
import { text } from "@language/text";
import Layout from "@layouts/Layout.astro";
import { baseUrl } from "@src/urls";

const breadcrumbs = [
  {
    title: text.minSide["nb"],
    url: "https://www.nav.no/minside",
  },
  {
    url: baseUrl,
    title: text.dokumentarkiv["nb"],
  },
];
---

<Layout title="Dokumentarkiv" breadcrumbs={breadcrumbs} language="nb">
  <OnePagerContainerV2 language="nb" client:only="react" />
</Layout>
